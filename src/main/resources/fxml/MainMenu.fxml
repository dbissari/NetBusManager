<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="center" prefHeight="600" prefWidth="800" spacing="15" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="fr.utbm.gl52.netbusmanager.controller.MainMenuController">

    <VBox alignment="center" maxHeight="600" maxWidth="400" prefHeight="400" spacing="15">
        <Button fx:id="mapScreenShowButton" onAction="#mapShow" text="Ouvrir l'aperçu du réseau" />
        <Button fx:id="stopEditorShowButton" onAction="#stopEditorShow" text="Gérer les arrêts" />
        <Button fx:id="routeEditorShowButton" onAction="#lineEditorShow" text="Créer une ligne / un itinéraire" />
        <Button fx:id="scheduleEditorButton" onAction="#scheduleEditorShow" text="Gérer les tables de marche" />
    </VBox>
    
</VBox>
